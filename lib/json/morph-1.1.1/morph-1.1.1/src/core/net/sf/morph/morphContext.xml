<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans
	default-lazy-init="true">

<!--============================================================================
|
| REFLECTORS
|
=============================================================================-->

	<bean
		name="listReflector"
		class="net.sf.morph.reflect.reflectors.ListReflector"/>

	<bean
		name="sortedSetReflector"
		class="net.sf.morph.reflect.reflectors.SortedSetReflector"/>

	<bean
		name="setReflector"
		class="net.sf.morph.reflect.reflectors.SetReflector"/>

	<bean
		name="enumerationReflector"
		class="net.sf.morph.reflect.reflectors.EnumerationReflector"/>

	<bean
		name="iteratorReflector"
		class="net.sf.morph.reflect.reflectors.IteratorReflector"/>

	<bean
		name="arrayReflector"
		class="net.sf.morph.reflect.reflectors.ArrayReflector"/>

	<bean
		name="collectionReflector"
		class="net.sf.morph.reflect.reflectors.CollectionReflector"/>

	<bean
		name="resultSetReflector"
		class="net.sf.morph.reflect.reflectors.ResultSetReflector"/>

	<bean
		name="contextReflector"
		class="net.sf.morph.reflect.reflectors.ContextReflector"/>

	<bean
		name="mapReflector"
		class="net.sf.morph.reflect.reflectors.MapReflector"/>

	<bean
		name="servletContextAttributeReflector"
		class="net.sf.morph.reflect.reflectors.ServletContextAttributeReflector"/>

	<bean
		name="httpSessionAttributeReflector"
		class="net.sf.morph.reflect.reflectors.HttpSessionAttributeReflector"/>

	<bean
		name="servletRequestAttributeReflector"
		class="net.sf.morph.reflect.reflectors.ServletRequestAttributeReflector"/>

	<bean
		name="pageContextAttributeReflector"
		class="net.sf.morph.reflect.reflectors.PageContextAttributeReflector"/>

	<bean
		name="dynaBeanReflector"
		class="net.sf.morph.reflect.reflectors.DynaBeanReflector"/>

	<bean
		name="objectReflector"
		class="net.sf.morph.reflect.reflectors.ObjectReflector"/>

	<bean
		name="simpleInstantiatingReflector"
		class="net.sf.morph.reflect.reflectors.SimpleInstantiatingReflector"/>

	<bean
		name="delegatingReflector"
		class="net.sf.morph.reflect.reflectors.SimpleDelegatingReflector"/>

<!--============================================================================
|
| TRANSFORMERS
|
=============================================================================-->

	<bean
		name="defaultToBooleanConverter"
		class="net.sf.morph.transform.converters.DefaultToBooleanConverter"/>

	<bean
		name="nullConverter"
		class="net.sf.morph.transform.converters.DefaultToTextConverter"/>

	<bean
		name="identityConverter"
		class="net.sf.morph.transform.converters.IdentityConverter"/>

	<bean
		name="immutableTypesOnlyIdentityConverter"
		class="net.sf.morph.transform.converters.ImmutableTypesOnlyIdentityConverter"/>

	<bean
		name="defaultToTextConverter"
		class="net.sf.morph.transform.converters.DefaultToTextConverter"/>

	<bean
		name="textToNumberConverter"
		class="net.sf.morph.transform.converters.TextToNumberConverter"/>

	<bean
		name="textToTimeConverter"
		class="net.sf.morph.transform.converters.TextToTimeConverter"/>

	<bean
		name="numberToTimeConverter"
		class="net.sf.morph.transform.converters.NumberToTimeConverter"/>

	<bean
		name="timeToNumberConverter"
		class="net.sf.morph.transform.converters.TimeToNumberConverter"/>

	<bean
		name="numberConverter"
		class="net.sf.morph.transform.converters.NumberConverter"/>

	<bean
		name="textConverter"
		class="net.sf.morph.transform.converters.TextConverter"/>

	<bean
		name="timeConverter"
		class="net.sf.morph.transform.converters.TimeConverter"/>

	<bean
		name="containerCopier"
		class="net.sf.morph.transform.copiers.ContainerCopier"/>

	<bean
		name="propertyNameMatchingCopier"
		class="net.sf.morph.transform.copiers.PropertyNameMatchingCopier"/>

	<bean
		name="delegatingTransformer"
		class="net.sf.morph.transform.transformers.SimpleDelegatingTransformer"/>

<!--============================================================================
|
| MISC
|
=============================================================================-->

</beans>